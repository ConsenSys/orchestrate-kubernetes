---
fullnameOverride: redis-nonce
usePassword: false
master:
  persistence:
    enabled: false
configmap: |-
  notify-keyspace-events Kg
  maxmemory 100mb
  maxmemory-policy allkeys-lru
cluster:
  enabled: false
metrics:
  enabled: {{ .Values | get "metrics.enabled" false }}
  serviceMonitor:
    enabled: {{ .Values | get "metrics.enabled" false }}
    namespace: {{ .Values | get "observabilityNamespace" }}
